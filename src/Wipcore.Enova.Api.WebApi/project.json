{
  "version": "6.4.0.215-dev",
  "dependencies": {
    "Autofac": "4.1.1",
    "Autofac.Extensions.DependencyInjection": "4.0.0",
    "IdentityServer4": "1.0.0-rc5",
    "Microsoft.AspNetCore.Authentication.Cookies": "1.1.0",
    "Microsoft.AspNetCore.Authentication.JwtBearer": "1.1.0",
    "Microsoft.AspNetCore.DataProtection.Extensions": "1.1.0",
    "Microsoft.AspNetCore.Diagnostics": "1.1.0",
    "Microsoft.AspNetCore.Html.Abstractions": "1.1.0",
    "Microsoft.AspNetCore.Mvc": "1.1.0",
    "Microsoft.AspNetCore.Mvc.WebApiCompatShim": "1.1.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.1.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.0",
    "Microsoft.AspNetCore.StaticFiles": "1.1.0",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.1.0",
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Microsoft.Extensions.Logging.Debug": "1.1.0",
    "NLog": "4.4.0-*",
    "NLog.Extensions.Logging": "1.0.0-rtm-alpha4",
    "Swashbuckle.AspNetCore": "1.0.0",
    "Wipcore.Enova.Api.Abstractions": {
      "version": "6.4.0.215-*",
      "target": "project"
    },
    "Wipcore.Enova.Api.Abstractions.Internal": {
      "target": "project",
      "version": "6.4.0.215-*"
    },
    "Wipcore.Enova.Api.OAuth": {
      "version": "6.4.0.215-*",
      "target": "project"
    },
    "Wipcore.Enova.Connectivity.Package": "6.4.0.243-dev",
    "Wipcore.Enova.Core.Extensions.Package": "6.4.0.243-dev",
    "Wipcore.Enova.Core.Package": "6.4.0.243-dev"
  },

  "commands": {
    "web": "Microsoft.AspNetCore.Server.Kestrel"
  },

  "frameworks": {
    "net461": {
      "frameworkAssemblies": {
        "System.Runtime.Caching": "4.0.0.0"
      }
    }
  },
  "buildOptions": {
    "exclude": [
      "wwwroot",
      "node_modules"
    ],

    "emitEntryPoint": true,
    "xmlDoc": true,
    "nowarn": [ "1591" ]
  },
  "publishOptions": {

    "include": [
      "web.config",
      "Configs",
      "SwaggerDocs",
      "AddIn"
    ],

    "publishExclude": [
      "**.user",
      "**.vspscc"
    ]
  }
}
